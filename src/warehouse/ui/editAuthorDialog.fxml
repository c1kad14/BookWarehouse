<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="282.0" prefWidth="383.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="warehouse.controllers.EditAuthorDialogController">
    <children>
        <Label layoutX="42.0" layoutY="68.0" text="Id: "/>
        <TextField fx:id="idTextBox" disable="true" layoutX="151.0" layoutY="63.0"/>
        <Label layoutX="42.0" layoutY="115.0" prefHeight="21.0" prefWidth="88.0" text="First Name:"/>
        <TextField fx:id="firstNameTextBox" layoutX="151.0" layoutY="110.0"/>
        <Label layoutX="42.0" layoutY="162.0" prefHeight="21.0" prefWidth="88.0" text="Last Name:"/>
        <TextField fx:id="lastNameTextBox" layoutX="151.0" layoutY="157.0"/>
        <Button fx:id="closeBtn" layoutX="282.0" layoutY="218.0" mnemonicParsing="false" text="Close"
                onAction="#closeBtnClick"/>
        <Button fx:id="saveBtn" layoutX="209.0" layoutY="218.0" mnemonicParsing="false" text="Save"
                onAction="#saveBtnClick"/>
    </children>
</AnchorPane>
